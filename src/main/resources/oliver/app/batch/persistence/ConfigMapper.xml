<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="oliver.app.batch.persistence.ConfigMapper">

    <cache/>

    <sql id="configFullColumn">
        id,namespace,name,value,mod_time as modTime,add_time as addTime,note
    </sql>

    <select id="findAll" resultType="Config">
        select
        <include refid="configFullColumn"/>
        from config
    </select>

</mapper>